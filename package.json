{
  "name": "@jarvis-home-automation/trainerbot",
  "version": "1.0.0",
  "description": "To train neural network models and save them for brain.js",
  "main": "train.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run:dev": "npm run compile:dev && node ./dist/dev.bundle.js",
    "compile:dev": "npx webpack --config webpack.config.js --env development",
    "compile:prod": "npx webpack --config webpack.config.js --env production",
    "deploy": "npm run compile:prod && nexe dist/prod.bundle.js -o deploy/trainerbot.exe --build --target \"windows-x64-18.17.1\"",
    "run:prod": "npm run compile:prod && node ./dist/prod.bundle.js",
    "format": "prettier --write \"train.js\"",
    "lint": "eslint \"train.js\"",
    "lint:fix": "eslint --fix \"train.js\""
  },
  "keywords": [
    "train",
    "bot",
    "brain",
    "brain.js"
  ],
  "author": "Parker Bidigare",
  "license": "ISC",
  "dependencies": {
    "arg": "^5.0.1",
    "brain.js": "^2.0.0-beta.11",
    "cli-progress": "^3.12.0",
    "fs": "^0.0.1-security",
    "google-closure-compiler": "^20240317.0.0",
    "webgl": "^0.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.0.0",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "html-webpack-plugin": "^5.6.0",
    "json-loader": "^0.5.7",
    "nexe": "^4.0.0-rc.4",
    "node-loader": "^2.0.0",
    "prettier": "^3.2.5",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  }
